#include "nfc_driver.h"
#include "print_driver.h"
#include <iostream>
#include "sector.h"

int main()
{
	Printer p(true);
	NfcDevice   nfc(&p);
	Card		*tmp = nullptr;

	tmp = nfc.findCard();
	tmp->readSector(1);
	return 0;
	(*tmp)[2].useKeyB(true);
	(*tmp).readSector(1);
	uint8_t data[6] = {0xaa, 0xaa, 0xaa, 0xaa, 0xaa, 0xaa};
	(*tmp)[1].setKeyB(data);
	tmp->writeSector(1);
	return 0;
}
